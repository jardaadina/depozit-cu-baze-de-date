CREATE DEFINER=`root`@`localhost` PROCEDURE `insertProduct`(
    IN pID INT,
    IN p_Name VARCHAR(45),
    IN p_Price float,
    in p_stock int
)
BEGIN
    DECLARE product_id INT;
    
    -- Check if the product already exists
    SELECT productID INTO product_id FROM products WHERE productID = pID;
    
    IF product_id IS NULL THEN
        -- If the product doesn't exist, insert a new record
        INSERT INTO products (productID, name, stock, price) VALUES (pID, p_Name, p_stock, p_Price);
    ELSE
        -- If the product exists, update the stock (or any other details if needed)
        UPDATE products SET stock = stock + p_stock WHERE productID = pID;
    END IF;
END ;;
DELIMITER ;
