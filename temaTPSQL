CREATE DEFINER=`root`@`localhost` PROCEDURE `createOrder`(
    IN cID INT,
    IN pID INT,
    IN q INT
)
BEGIN
    DECLARE available_stock INT;
    DECLARE product_price FLOAT;
    DECLARE total_price DOUBLE;

    -- Get available stock and product price directly
    SELECT stock, price INTO available_stock, product_price
    FROM product WHERE productID = pID
    LIMIT 1;

    IF available_stock >= q THEN
        SET total_price = q * product_price;
        -- Create order
        INSERT INTO orders (ClientID, ProductID, totalPrice, quantity)
        VALUES (cID, pID, total_price, q);
        -- Update product stock
        UPDATE product SET stock = stock - q WHERE productID = pID;

        SELECT 'Order created successfully' AS Message;
    ELSE
        SELECT 'Insufficient stock' AS Message;
    END IF;
END ;;
DELIMITER ;
